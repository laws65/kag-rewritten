[application]

config/name="KAG Rewritten"
run/main_scene="res://base/levels/content/matchmaking.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="KAG Rewritten"
boot_splash/image="res://base/sprites/misc/icon.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://base/sprites/misc/icon.png"

[autoload]

game_map="*res://base/levels/content/misc/game_map.tscn"
game_camera="*res://base/levels/content/misc/game_camera.tscn"
game_chat="*res://base/levels/content/misc/game_chat.tscn"
game_fps="*res://base/levels/content/misc/game_fps.tscn"
network="*res://base/levels/content/misc/network.tscn"

[debug]

settings/crash_handler/message="Please include this when reporting the bug on https://github.com/kag-rewritten/kag-rewritten/issues"
gdscript/warnings/unused_argument=false
gdscript/warnings/return_value_discarded=false

[display]

window/dpi/allow_hidpi=true
window/vsync/use_vsync=false
mouse_cursor/custom_image="res://base/sprites/misc/cursor.png"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[gui]

theme/custom="res://base/themes/default_theme.tres"
theme/custom_font="res://base/themes/fonts/m5x7.tres"

